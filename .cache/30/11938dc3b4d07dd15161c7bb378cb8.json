e":"style/style1.scss","name":null,"original":{"line":250,"column":8},"generated":{"line":248,"column":4}},{"source":"style/style1.scss","name":null,"original":{"line":250,"column":8},"generated":{"line":248,"column":13}},{"source":"style/style1.scss","name":null,"original":{"line":251,"column":8},"generated":{"line":249,"column":4}},{"source":"style/style1.scss","name":null,"original":{"line":251,"column":8},"generated":{"line":249,"column":36}}],"sources":{"style/style1.scss":"* {\n    margin: 0px;\n    padding: 0px;\n    box-sizing: border-box;\n    font-family: sans-serif;\n    font-family: 'Montserrat', sans-serif;\n}\n\nbody {\n    font-size: 20px;\n    overflow-x: hidden;\n    color: #fff;\n    overflow: hidden;\n}\n\nheader {\n    background-image:linear-gradient(rgba(0,0,0,0.5), rgba(0,0,0,0.7)), url('../img/pozadina.png');\n    height: 100vh;\n    background-repeat: no-repeat;\n    background-size: cover;\n}\n\n.transition-fade {\n    transition: 0.4s; \n    opacity: 1;\n}\n\n.html.is-animating .transition-fade {\n    opacity: 0;\n    transform: translateY(-100px);\n}\n\n.clearfix:after {\n    content: \".\";\n    visibility: hidden;\n    display: block;\n    height: 0px;\n    clear: both;\n}\n\n.row {\n    background: rgba(0,0,0,0.5);\n    border-bottom: 3px solid #b80909;\n}\n\n.logo {\n    height: 120px;\n    width: auto;\n    float: left;\n    margin-top: 10px;\n    margin-left: 10px;\n}\n\n.main-nav {\n    float: right;\n    margin-top: 60px;\n}\n\n.main-nav li {\n    display: inline-block;\n    list-style: none;\n    margin-left: 80px;\n    margin-right: 20px;\n}\n\n.main-nav ul {\n    margin-left: 80px;\n}\n\n.main-nav li a {\n    padding: 5px 0;\n    color: #fff;\n    text-decoration: none;\n    text-transform: uppercase;\n    font-size: 90%;\n    font-weight: 100;\n}\n\n.main-nav li a:hover {\n    border-bottom: 2px solid #b80909;\n}\n\n.main-content {\n    width: 1180px;\n    position: absolute;\n    top: 60%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n}\n\nh1 {\n    color: #fff;\n    font-size: 150%;\n    word-spacing: 5px;\n    letter-spacing: 3px;\n    margin-bottom: 20px;\n    text-transform: uppercase;\n    font-weight: lighter;\n\n}\n\n.btn {\n    display: inline-block;\n    padding: 10px 30px;\n    font-weight: lighter;\n    text-decoration: none;\n    text-transform: uppercase;\n    border-radius: 200px;\n    transition: background-color 0.2s, border 0.2s, color 0.2;\n}\n\n.btn-full {\n    background-color: #fff;\n    color: #b80909;\n    margin-right: 25px;\n    border: 1px solid #b80909;\n    transition: 0.5s;\n}\n\n.btn-full:hover {\n    background-color: #b80909;\n    color: #fff;\n    border: 1px solid #fff;\n}\n\n.btn-nav {\n    background-color: transparent;\n    color: #fff;\n    border: 1px solid #fff;\n    transition: 0.5s;\n}\n\n.btn-nav:hover {\n    background-color: #b80909;\n}\n\n.active {\n    border-bottom: 2px solid #1ab8e8;\n    color: #1ab8e8;\n}\n\n.naslov {\n    animation: colorchange 1s infinite;\n}\n\n.social-icons {\n    right: 22px;\n    bottom: 11px;\n    position: fixed;\n}\n\n.social-icons ul li {\n    list-style: none;\n    margin-top: 15px;\n    text-align: center;\n}\n\n.social-icons ul li a {\n    color: #b80909;\n    border-radius: 50%;\n    border: 1px solid #b80909;\n    padding: 5px;\n    display: block;\n    background-color: white;\n    transition: 0.5s;\n}\n\n.social-icons ul li a:hover {\n    background-color: #b80909;\n    color: #fff;\n    border: 1px solid white;\n}\n\n@keyframes colorchange {\n    0% {color: #fff;}\n    50% {color: #b80909;\n        text-shadow: -1px 0 white, 0 1px white, 1px 0 white, 0 -1px white;\n    }\n    100% {color: #fff;}\n}\n\n.mobile-icon {\n    display: none;\n}\n\n/*RESPONZIVNOST*/\n\n@media only screen and (max-width: 1180px) {\n    .main-content {\n        width: 100%;\n        padding: 0 2%;\n    }\n}\n\n@media only screen and (max-width: 998px) {\n    h1 {\n        font-size: 120%;\n    }\n\n    .main-nav li {\n        display: inline-block;\n        list-style: none;\n        margin-left: 20px;\n        margin-right: 10px;\n    }\n}\n\n@media only screen and (max-width: 768px) {\n    h1 {\n        font-size: 110%;\n    }\n\n    .main-nav{\n        display: none;\n    }\n\n    .mobile-icon {\n        display: inline-block;\n        color: white;\n        float: right;\n        margin-top: 30px;\n        margin-right: 20px;\n    }\n\n    .main-nav {\n        float: left;\n    }\n\n    .main-nav li {\n        display: block;\n        margin-top: 10px;\n    }\n}\n\n@media only screen and (max-width: 768px) {\n    .btn-full {\n        margin-bottom: 10px;\n    }\n\n    h1 {\n        font-size: 100%;\n    }\n}\n\n@media only screen and (max-height: 450px) {\n    .main-content {\n        width: 1180px;\n        position: absolute;\n        top: 80%;\n        left: 70%;\n        transform: translate(-50%, -50%);\n    }\n}\n\n\n\n"},"lineCount":null}},"error":null,"hash":"08fd84ac19fa3e08fe758ef3a4f7f5a4","cacheData":{"env":{}}}